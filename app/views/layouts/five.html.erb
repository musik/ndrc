        </script>
    </head>
    
    <body class="newcrop">
        <div id="topbarx">
        </div>
        <div id="homeWrap" class="wrapper">
            <div id="header">
                <div id="headerinside">
                    <a href="http://static.58.com/listed/index.html" id="logo" target="_target">
                      <img src="/images/logo.png" alt="<%=t('appname')%>" title="<%=t('appname')%>"
                        width="137" height="52">
                    </a>
                    <form action="/search" target="_self" method="get">                        <div id="searchbar">
                            <div id="saerkey">
                                <span id="key">
                                    <input type="text" id="keyword" name="q" class="keyword" value="请输入类别名称或关键字"
                                    onBlur="win.closeToolTipWindow();if(this.value=='')this.value='请输入类别名称或关键字',this.className='keyword'"
                                    onKeyUp="win.GetContentData();" onFocus="if(this.value=='请输入类别名称或关键字')this.value='',this.className='keyword2'"
                                    autocomplete="off" />
                                </span>
                            </div>
                            <div class="inputcon">
                                <input type="submit" name="button" id="searchbtn" class="btnall" value="企业搜索"
                                onMouseMove="this.className='btnal2'" onMouseOut="this.className='btnall'"
                                />
                            </div>
                            <div class="clear">
                            </div>
                            <div class="search-no">
                                <span id="hot">
                                </span>
                                <span class="hot2">
                                </span>
                            </div>
                        </div>
                    </form>
                    <%#<a href="/post" id="fabu" rel="nofollow">%>
                      <%#<i></i>免费发布信息%>
                    <%#</a>%>
                </div>
            </div>
            <div class="hShadow">
            </div>
            <div class="navcon" id="nav">
                <ul class="nav">
                    <li class="on">
                        <a href="/" target="_self">
                            首页
                        </a>
                    </li>
                    <% Cat.all.each do |cat|%>
                      <li><a href="/<%=cat.slug%>"><%=cat.name%></a></li>
                    <%end %>
                    
                </ul>
                <ul class="rightside">
                </ul>
                <div class="ad_nav">
                </div>
            </div>
        </div>
        <%=yield %>
        <div class="morelink">
        </div>
        <div id="footer">
            <div class="bg">
                <div id="contact">
                    <a href="http://sighttp.qq.com/authd?IDKEY=f62e57b812e5ba6bcabb95e781a841ed8edb5efa46c90997"  title="有问题请与客服进行对话" class="fduihua" rel="nofollow">
                        客服
                    </a>
                    <span>|</span>
                    <a href="/zeig" target="_self">网站地图</a>
                    <span>|</span>
                    <a href="/qiye/recent" target="_self">最新索引</a>
                    <span>|</span>
                    <a href="/topics/recent" target="_self">最新搜索</a>
                    <br />
                    &copy; www.ndrc.ac.cn
                    <a href="http://www.miibeian.gov.cn/" style="color:#666" rel="nofollow">
                      京ICP备05050413号
                    </a>
                    <%=Time.now%>
                </div>
            </div>
        </div>
    </body>

</html>
